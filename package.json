{
	"name": "wikibase-vuejs-components",
	"version": "0.1.0",
	"description": "A library for vuejs components used in Wikibase projects. Inspired by OOUI",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/wmde/wikibase-vuejs-components.git"
	},
	"bugs": {
		"url": "https://github.com/wmde/wikibase-vuejs-components/issues"
	},
	"homepage": "https://github.com/wmde/wikibase-vuejs-components#readme",
	"keywords": [
		"vue",
		"vuejs",
		"OOUI",
		"Mediawiki",
		"Wikibase",
		"components"
	],
	"author": "The Wikidata team",
	"license": "GPL-2.0+",
	"scripts": {
		"build:components": "NODE_ENV=production vue-cli-service build --target lib --formats commonjs --name wikibase-vuejs-components src/index.ts",
		"build:types": "tsc -p tsconfig.prod.json",
		"build": "npm-run-all build:*",
		"fix": "vue-cli-service lint --max-warnings 0 .",
		"test": "npm-run-all test:*",
		"test:lint": "vue-cli-service lint --max-warnings 0 --no-fix . && stylelint --syntax scss 'src/**/*.(vue|scss)'",
		"test:unit": "vue-cli-service test:unit -c ./jest.config.js",
		"test:distnodiff": "npm run build:components -- --dest /tmp/dist && diff -q ./dist/wikibase-vuejs-components.common.js /tmp/dist/wikibase-vuejs-components.common.js",
		"prepare": "node build/wikimedia-ui-base.js",
		"prepublishOnly": "npm run build"
	},
	"main": "./dist/wikibase-vuejs-components.common.js",
	"style": "./dist/wikibase-vuejs-components.css",
	"typings": "./dist/index.d.ts",
	"devDependencies": {
		"@types/jest": "^23.1.4",
		"@typescript-eslint/eslint-plugin": "^1.13.0",
		"@typescript-eslint/parser": "^1.13.0",
		"@vue/cli-plugin-babel": "^3.12.0",
		"@vue/cli-plugin-eslint": "^3.12.0",
		"@vue/cli-plugin-unit-jest": "^3.12.0",
		"@vue/cli-service": "^3.12.0",
		"@vue/eslint-config-typescript": "^4.0.0",
		"@vue/test-utils": "1.0.0-beta.29",
		"babel-core": "7.0.0-bridge.0",
		"babel-eslint": "^10.0.3",
		"babel-loader": "^8.0.6",
		"babel-preset-vue": "^2.0.2",
		"eslint": "^5.16.0",
		"eslint-config-wikimedia": "^0.13.1",
		"eslint-plugin-filenames": "^1.3.2",
		"eslint-plugin-vue": "^5.2.3",
		"less-variable-resolver": "^0.1.2",
		"gonzales-pe": "^4.2.4",
		"node-sass": "^4.12.0",
		"npm-run-all": "^4.1.5",
		"postcss-prefixwrap": "^1.7.0",
		"sass-loader": "^7.3.1",
		"sast": "^0.8.1",
		"stylelint": "^10.1.0",
		"stylelint-config-wikimedia": "^0.6.0",
		"stylelint-processor-html": "^1.0.0",
		"stylelint-scss": "^3.11.1",
		"typescript": "3.5.3",
		"ts-jest": "^23.10.5",
		"vue": "^2.6.10",
		"vue-class-component": "^6.0.0",
		"vue-cli-plugin-typescript": "0.0.1",
		"vue-property-decorator": "^7.0.0",
		"vue-template-compiler": "^2.6.10",
		"wikimedia-ui-base": "^0.14.0"
	},
	"peerDependencies": {
		"vue": "^2.6.10",
		"vue-property-decorator": "^7.0.0",
		"vue-class-component": "^6.0.0"
	}
}
